    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "https://localhost:3000",
            "webRoot": "${workspaceFolder}/WebApp"
        },
        {
            "name": "run from vs code",
            "type": "node-terminal",
            "request": "launch",
            "command": "npm run startFromVsCode",
        }
    ]
}